{
  "name": "woffu",
  "version": "1.0.0",
  "description": "Woffu",
  "main": "index.js",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint \"src/**/*.{js,jsx}\"",
    "stylelint": "./node_modules/.bin/stylelint \"src/**/*.{js,jsx}\"",
    "format": "npm run prettier -- --write",
    "prettier": "./node_modules/.bin/prettier \"src/**/*.{js,jsx,css,json}\"",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "dev": "npm run webpack -- --env.mode development",
    "prod": "npm run webpack -- --env.mode production",
    "prod:analyze": "npm run prod -- --env.presets analyze",
    "prod:compress": "npm run prod -- --env.presets compress",
    "start": "npm run webpack-dev-server -- --env.mode development --watch --open --hot"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "extends": [
      "./node_modules/lyra-lint/.eslintrc.js"
    ]
  },
  "stylelint": {
    "extends": "./node_modules/lyra-lint/stylelint.config.js"
  },
  "prettier": "./node_modules/lyra-lint/.prettierrc.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --max-warnings=0"
    ],
    "**/*.{js,jsx,css,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "lyra-lint": "^1.4.0",
    "mini-css-extract-plugin": "^2.7.2",
    "polished": "^3.4.4",
    "prop-types": "^15.7.2",
    "react-datepicker": "^2.12.1",
    "rxjs": "^6.5.4",
    "style-loader": "^1.3.0",
    "styled-components": "^5.0.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
